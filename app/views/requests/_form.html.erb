<%= form_for [@user, @request] do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :date %>
<%= f.date_field :day %>

<%= f.label :start_time %>
<%= f.text_field :start_time %>

<%= f.label :duration %>
<%= f.text_field :duration %>

<%= f.label :location %>
<%= f.text_field :location %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.submit %>

<% end %>
